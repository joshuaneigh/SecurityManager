<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="600.0" stylesheets="/view/application.css" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.stage.Dialog">
   <children>
      <HBox>
         <VBox.margin>
            <Insets bottom="5.0" />
         </VBox.margin>
         <children>
            <ImageView fx:id="icon" fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin></ImageView>
            <Text fx:id="message" onMouseDragged="#handleMoved" onMousePressed="#handleMoveStarted" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" HBox.hgrow="ALWAYS">
               <font>
                  <Font size="18.0" />
               </font>
            </Text>
            <Region onMouseDragged="#handleMoved" onMousePressed="#handleMoveStarted" prefHeight="0.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
            <Region id="close-button" maxHeight="24.0" maxWidth="24.0" minHeight="24.0" minWidth="24.0" onMouseClicked="#handleCancel" prefHeight="24.0" prefWidth="24.0" />
         </children>
      </HBox>
      <Separator prefWidth="200.0" />
      <TextArea id="transparent-text-area" fx:id="description" editable="false" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
      <ButtonBar prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button mnemonicParsing="false" onAction="#handleOkay" text="Continue" />
            <Button mnemonicParsing="false" onAction="#handleQuit" text="Terminate" />
        </buttons>
         <VBox.margin>
            <Insets top="5.0" />
         </VBox.margin>
      </ButtonBar>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
